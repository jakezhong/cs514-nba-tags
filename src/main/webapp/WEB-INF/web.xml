<!-- [START webxml] -->
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
         version="2.5">
    <!--
      A web.xml is needed to explicitly set the order in which filters process requests. Any filters
      not included in web.xml will be loaded after filters listed below.
    -->
    <filter>
      <filter-name>DatastoreSessionFilter</filter-name>
      <filter-class>com.example.getstarted.util.DatastoreSessionFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>DatastoreSessionFilter</filter-name>
        <url-pattern>/login</url-pattern>
        <url-pattern>/logout</url-pattern>
        <url-pattern>/</url-pattern>
        <url-pattern>/persons</url-pattern>
        <url-pattern>/persons/mine</url-pattern>
        <url-pattern>/person/create</url-pattern>
        <url-pattern>/person/delete</url-pattern>
        <url-pattern>/person/read</url-pattern>
        <url-pattern>/person/update</url-pattern>
        <url-pattern>/person/groups</url-pattern>
        <url-pattern>/groups</url-pattern>
        <url-pattern>/groups/mine</url-pattern>
        <url-pattern>/group/create</url-pattern>
        <url-pattern>/group/delete</url-pattern>
        <url-pattern>/group/read</url-pattern>
        <url-pattern>/group/update</url-pattern>
        <url-pattern>/group/members</url-pattern>
        <url-pattern>/association/create</url-pattern>
        <url-pattern>/posts</url-pattern>
        <url-pattern>/posts/mine</url-pattern>
        <url-pattern>/post/create</url-pattern>
        <url-pattern>/post/delete</url-pattern>
        <url-pattern>/post/read</url-pattern>
        <url-pattern>/post/update</url-pattern>
        <url-pattern>/user</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>LogoutFilter</filter-name>
        <filter-class>com.example.getstarted.auth.LogoutFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>LogoutFilter</filter-name>
        <url-pattern>/logout</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>ListByUserFilter</filter-name>
        <filter-class>com.example.getstarted.auth.ListPersonByUserFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ListByUserFilter</filter-name>
        <url-pattern>/persons/mine</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>ListGroupByUserFilter</filter-name>
        <filter-class>com.example.getstarted.auth.ListGroupByUserFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ListGroupByUserFilter</filter-name>
        <url-pattern>/groups/mine</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>com.example.getstarted.auth.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.example.getstarted.auth.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListPerson</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ListPersonServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListPerson</servlet-name>
        <url-pattern>/</url-pattern>
        <url-pattern>/persons</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListPersonByUser</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ListPersonByUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListPersonByUser</servlet-name>
        <url-pattern>/persons/mine</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreatePerson</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.CreatePersonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreatePerson</servlet-name>
        <url-pattern>/person/create</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeletePerson</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.DeletePersonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeletePerson</servlet-name>
        <url-pattern>/person/delete</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReadPerson</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ReadPersonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReadPerson</servlet-name>
        <url-pattern>/person/read</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdatePerson</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.UpdatePersonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdatePerson</servlet-name>
        <url-pattern>/person/update</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListGroupByPerson</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ListGroupByPersonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListGroupByPerson</servlet-name>
        <url-pattern>/person/groups</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListGroup</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ListGroupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListGroup</servlet-name>
        <url-pattern>/groups</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListGroupByUser</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ListGroupByUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListGroupByUser</servlet-name>
        <url-pattern>/groups/mine</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateGroup</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.CreateGroupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateGroup</servlet-name>
        <url-pattern>/group/create</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReadGroup</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ReadGroupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReadGroup</servlet-name>
        <url-pattern>/group/read</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteGroup</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.DeleteGroupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteGroup</servlet-name>
        <url-pattern>/group/delete</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateGroup</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.UpdateGroupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateGroup</servlet-name>
        <url-pattern>/group/update</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListPersonByGroup</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ListGroupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListPersonByGroup</servlet-name>
        <url-pattern>/group/members</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateAssociation</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.CreateAssociationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateAssociation</servlet-name>
        <url-pattern>/association/create</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListPost</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ListPostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListPost</servlet-name>
        <url-pattern>/posts</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListPostByUser</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ListPostByUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListPostByUser</servlet-name>
        <url-pattern>/posts/mine</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreatePost</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.CreatePostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreatePost</servlet-name>
        <url-pattern>/post/create</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReadPost</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ReadPostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReadPost</servlet-name>
        <url-pattern>/post/read</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeletePost</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.DeletePostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeletePost</servlet-name>
        <url-pattern>/post/delete</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdatePost</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.UpdatePostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdatePost</servlet-name>
        <url-pattern>/post/update</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReadUser</servlet-name>
        <servlet-class>com.example.getstarted.basicactions.ReadUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReadUser</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>

    <!-- [START config] -->
    <context-param>
        <param-name>personshelf.storageType</param-name>
        <param-value>${personshelf.storageType}</param-value>
    </context-param>

    <context-param>
        <param-name>personshelf.bucket</param-name>
        <param-value>${personshelf.bucket}</param-value>
    </context-param>

    <context-param>
        <param-name>sql.urlRemoteGAE</param-name>
        <param-value>jdbc:google:mysql://${sql.instanceName}/${sql.dbName}?ourUser=${sql.userName}&amp;password=${sql.password}</param-value>
    </context-param>

    <context-param>
        <param-name>sql.urlRemote</param-name>
        <param-value>jdbc:mysql://google/${sql.dbName}?cloudSqlInstance=${sql.instanceName}&amp;socketFactory=com.google.cloud.sql.mysql.SocketFactory&amp;ourUser=${sql.userName}&amp;password=${sql.password}</param-value>
    </context-param>

    <context-param>
        <param-name>sql.urlLocal</param-name>
        <param-value>jdbc:mysql://localhost/${sql.dbName}?ourUser=${sql.userName}&amp;password=${sql.password}&amp;useSSL=false</param-value>
    </context-param>
    <!-- [END config] -->
</web-app>
<!-- [END webxml] -->
